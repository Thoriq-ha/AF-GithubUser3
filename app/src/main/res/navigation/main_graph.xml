<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.thor.githubuser3.UI.Home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_homeFragment_to_DetailUserFragment"
            app:destination="@id/DetailUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/DetailUserFragment"
        android:name="com.thor.githubuser3.UI.DetailUser.DetailUserFragment"
        android:label="fragment_detail_user"
        tools:layout="@layout/fragment_detail_user">
        <action
            android:id="@+id/action_DetailUserFragment_self"
            app:destination="@id/DetailUserFragment" />
    </fragment>
</navigation>